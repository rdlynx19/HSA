<mujoco model="hsaModel">
    <compiler angle="radian" />
    <option timestep="0.001" actuatorgroupdisable="0 1 2" />
    
    <asset>
        <texture name="grid" type="2d" builtin="checker" width="500" height="500"
             rgb1="0.8 0.8 0.8" rgb2="0.5 0.5 0.5" mark="edge" markrgb="0.2 0.2 0.2"/>
        <material name="gridmat" texture="grid" texrepeat="5 5" reflectance="0"/> 
        <hfield name="terrain" size="5 5 0.5 0.01" nrow="50" ncol="50"/>  
    </asset>

    <worldbody>
        <light name="light_front" pos="0 -5 0" dir="0 1 0" directional="true" diffuse=".5 .5 .5"/>
        <light name="light_back" pos="0 5 0" dir="0 -1 0" directional="true" diffuse=".5 .5 .5"/>
        <light name="light_right" pos="5 0 0" dir="-1 0 0" directional="true" diffuse=".5 .5 .5"/>
        <light name="light_left" pos="-5 0 0" dir="1 0 0" directional="true" diffuse=".5 .5 .5"/>

        <geom name="floor" type="hfield" hfield="terrain" size="10 10 0.1" material="gridmat"/>

        

        <!-- Block A -->
        <body name="block_a" pos="-0.2 0 0.2">
            <freejoint/>
            <inertial pos="0.04 0 0" mass="2.4" diaginertia="0.001 0.01 0.01"/>
            <geom type="box" size="0.03 0.12 0.12" mass="2.4" friction="0.2 0.02 0.01" rgba="0.2 0.6 0.9 1"/>

            <!-- Cylinder 1A -->
            <body name="cylinder1a" pos="0.03 0.10 0.10">
                <inertial pos="0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_cont_1a_top" type="hinge" axis="1 0 0"  damping="0.1" />
                <geom name="cylinder1a_con" type="cylinder" fromto="0 0 0 0.15 0 0" size="0.04" rgba="0.9 0.05 0.15 1" />

                <site name="siteM1a1" pos="0.03 0.03 0.015"/>
                <site name="siteM1a2" pos="0.03 -0.03 -0.015"/>
            </body>

            <!-- Disc 1B -->
            <body name="disc1b" pos="0.175 0.10 0.10">
                <inertial pos="0.015 0 0" mass="0.1" diaginertia="0.0001 0.000215 0.000225"/>
                <geom name="disc1b_con" type="cylinder" fromto="0 0 0 0.02 0 0" size="0.17"  rgba="1 0 0 0.1" contype="0" conaffinity="0"/>
                <joint name="disc1b_hinge" type="hinge" axis="1 0 0"  damping="0.1"/>

                <site name="siteM1b1" pos="0 -0.165 -0.015"/>
                <site name="siteM1b2" pos="0 0.165 0.015"/>

                <site name="siteM1b3" pos="0.02 -0.165 0.015"/>
                <site name="siteM1b4" pos="0.02 0.165 -0.015"/>
            </body>

            
            <!-- Cylinder 2C-->
            <body name="cylinder2c" pos="0.03 -0.10 0.10">
                <inertial pos="0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_ext_2c_top" type="hinge" axis="1 0 0"  damping="0.1"/>
                <geom name="cylinder2c_con" type="cylinder" fromto="0 0 0 0.15 0 0" size="0.04" rgba="0.0 0.5 0.175 1"/>

                <site name="siteM2c1" pos="0.03 0.03 -0.015"/>
                <site name="siteM2c2" pos="0.03 -0.03 0.015"/>
            </body>


            <!-- Cylinder 3A -->
            <body name="cylinder3a" pos="0.03 -0.10 -0.10">
                <inertial pos="0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_cont_3a_btm" type="hinge" axis="1 0 0" damping="0.1"/>
                <geom name="cylinder3a_con" type="cylinder" fromto="0 0 0 0.15 0 0" size="0.04" rgba="0.9 0.9 0.02 1"/>
            
                <site name="siteM3a1" pos="0.03 0.03 0.015"/>
                <site name="siteM3a2" pos="0.03 -0.03 -0.015"/>
                
            </body>

            <!-- Disc 3B-->
            <body name="disc3b" pos="0.175 -0.10 -0.10">
                <inertial pos="0.015 0 0" mass="0.1" diaginertia="0.0001 0.000215 0.000225"/> 
                <geom name="disc3b_con" type="cylinder" fromto="0 0 0 0.02 0 0" size="0.17"  rgba="1 0 0 0.1" contype="0" conaffinity="0"/>
                <joint name="disc3b_hinge" type="hinge" axis="1 0 0"  damping="0.1"/>

                <site name="siteM3b1" pos="0.0 -0.165 -0.015"/>
                <site name="siteM3b2" pos="0.0 0.165 0.015"/>
                
                <site name="siteM3b3" pos="0.02 -0.165 0.015"/>
                <site name="siteM3b4" pos="0.02 0.165 -0.015"/>
            </body>

            <!-- Cylinder 4C-->
            <body name="cylinder4c" pos="0.03 0.10 -0.10">
                <inertial pos="0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_ext_4c_btm" type="hinge" axis="1 0 0" damping="0.1"/>
                <geom name="cylinder4c_con" type="cylinder" fromto="0 0 0 0.15 0 0" size="0.04" />
                <site name="siteM4c1" pos="0.03 0.03 -0.015"/>
                <site name="siteM4c2" pos="0.03 -0.03 0.015"/>
            </body>

        </body>

        <!-- Block B -->
        <body name="block_b" pos="0.2 0 0.2">
            <freejoint/>
            <inertial pos="-0.04 0 0" mass="2.4" diaginertia="0.001 0.01 0.01"/>
            <geom type="box" size="0.03 0.12 0.12" mass="2.4" friction="0.2 0.02 0.01" rgba="0.9 0.4 0.2 1"/>

            <!-- Cylinder 1C -->
            <body name="cylinder1c" pos="-0.03 0.10 0.10">
                <inertial pos="-0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_ext_1c_top" type="hinge" axis="1 0 0" damping="0.1"/>
                <geom name="cylinder1c_con" type="cylinder" fromto="0 0 0 -0.15 0 0" size="0.04" rgba="0.9 0.05 0.15 1"/>

                <site name="siteM1c1" pos="-0.03 0.03 -0.015"/>
                <site name="siteM1c2" pos="-0.03 -0.03 0.015"/>
            </body>

            <!-- Cylinder 3C-->
            <body name="cylinder3c" pos="-0.03 -0.10 -0.10">
                <inertial pos="-0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_ext_3c_btm" type="hinge" axis="1 0 0" damping="0.1"/>
                <geom name="cylinder3c_con" type="cylinder" fromto="0 0 0 -0.15 0 0" size="0.04" rgba="0.9 0.9 0.02 1"/>

                <site name="siteM3c1" pos="-0.03 0.03 -0.015"/>
                <site name="siteM3c2" pos="-0.03 -0.03 0.015"/>
            </body>

            <!-- Cylinder 2A -->
            <body name="cylinder2a" pos="-0.03 -0.10 0.10">
                <inertial pos="-0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_cont_2a_top" type="hinge" axis="1 0 0" damping="0.1"/>
                <geom name="cylinder2a_con" type="cylinder" fromto="0 0 0 -0.15 0 0" size="0.04" rgba="0.0 0.5 0.175 1"/>
        
                <site name="siteM2a1" pos="-0.03 0.03 0.015"/>
                <site name="siteM2a2" pos="-0.03 -0.03 -0.015"/>
            </body>
            
            <!-- Disc 2B -->
            <body name="disc2b" pos="-0.175 -0.10 0.10">
                <inertial pos="-0.015 0 0" mass="0.1" diaginertia="0.0001 0.000215 0.000225"/>
                <geom name="disc2b_con" type="cylinder" fromto="0 0 0 -0.02 0 0" size="0.17"  rgba="1 0 0 0.1" contype="0" conaffinity="0"/>
                <joint name="disc2b_hinge" type="hinge" axis="1 0 0"  damping="0.1"/>
               
                <site name="siteM2b1" pos="0 -0.165 -0.015"/>
                <site name="siteM2b2" pos="0 0.165 0.015"/> 

                <site name="siteM2b3" pos="-0.02 -0.165 0.015"/>
                <site name="siteM2b4" pos="-0.02 0.165 -0.015"/>       
            </body>



            <!-- Cylinder4A -->
            <body name="cylinder4a" pos="-0.03 0.10 -0.10">
                <inertial pos="-0.035 0 0" mass="0.1" diaginertia="0.0001 0.0002 0.0002"/>
                <joint name="cw_cont_4a_btm" type="hinge" axis="1 0 0"  damping="0.1"/>
                <geom name="cylinder4a_con" type="cylinder" fromto="0 0 0 -0.15 0 0" size="0.04" />

                <site name="siteM4a1" pos="-0.03 0.03 0.015"/>
                <site name="siteM4a2" pos="-0.03 -0.03 -0.015"/>
            </body>
            
            <!-- Disc4B -->
            <body name="disc4b" pos="-0.175 0.10 -0.10">
                <inertial pos="-0.015 0 0" mass="0.1" diaginertia="0.0001 0.000215 0.000225"/>
                <geom name="disc4b_con" type="cylinder" fromto="0 0 0 -0.02 0 0" size="0.17"  rgba="1 0 0 0.1" contype="0" conaffinity="0"/>
                <joint name="disc4b_hinge" type="hinge" axis="1 0 0"  damping="0.1"/>

                <site name="siteM4b1" pos="0 -0.165 -0.015"/>
                <site name="siteM4b2" pos="0 0.165 0.015"/>

                <site name="siteM4b3" pos="-0.02 -0.165 0.015"/>
                <site name="siteM4b4" pos="-0.02 0.165 -0.015"/>
            </body>

        </body>

        <!-- Goal Marker -->
        <site name="goal" pos="1.5 0.0 0.1" size="0.15" rgba="0 1 0 0.5"/>
    </worldbody>

    <!-- <equality>
        <joint name="lock_disc1b" joint1="disc1b_hinge" />
        <joint name="lock_disc2b" joint1="disc2b_hinge" />
        <joint name="lock_disc3b" joint1="disc3b_hinge" />
        <joint name="lock_disc4b" joint1="disc4b_hinge" />
    </equality> -->

    <contact>
        <!-- For friction, making the first 2 values different results in anisotropic tangential friction, last 2 results in anisotropic rolling friction -->
        <pair name="cylinder3a_ground" geom1="cylinder3a_con" geom2="floor" condim="3" friction="0.1 0.21 0.0005 0.21 0.0001"/>
        <pair name="cylinder3c_ground" geom1="cylinder3c_con" geom2="floor" condim="3" friction="0.1 0.21 0.0005 0.21 0.0001"/>
        <pair name="cylinder4a_ground" geom1="cylinder4a_con" geom2="floor" condim="3" friction="0.1 0.21 0.0005 0.21 0.0001"/>
        <pair name="cylinder4c_ground" geom1="cylinder4c_con" geom2="floor" condim="3" friction="0.1 0.21 0.0005 0.21 0.0001"/>
    </contact>

    <actuator>
        <motor name="spring1a_torque" group="0" joint="cw_cont_1a_top" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring2a_torque" group="0" joint="cw_cont_2a_top" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring3a_torque" group="0" joint="cw_cont_3a_btm" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring4a_torque" group="0" joint="cw_cont_4a_btm" gear="0.05" ctrlrange="-5 5"/>

        <motor name="spring1c_torque" group="0" joint="cw_ext_1c_top" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring2c_torque" group="0" joint="cw_ext_2c_top" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring3c_torque" group="0" joint="cw_ext_3c_btm" gear="0.05" ctrlrange="-5 5"/>
        <motor name="spring4c_torque" group="0" joint="cw_ext_4c_btm" gear="0.05" ctrlrange="-5 5"/>
    </actuator>
    <actuator>
        <position name="spring1a_motor" group="1" joint="cw_cont_1a_top" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring2a_motor" group="1" joint="cw_cont_2a_top" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring3a_motor" group="1" joint="cw_cont_3a_btm" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring4a_motor" group="1" joint="cw_cont_4a_btm" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>

        <position name="spring1c_motor" group="1" joint="cw_ext_1c_top" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring2c_motor" group="1" joint="cw_ext_2c_top" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring3c_motor" group="1" joint="cw_ext_3c_btm" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
        <position name="spring4c_motor" group="1" joint="cw_ext_4c_btm" kp="1"  ctrlrange="-3141.5 3141.5" kv="0.1"/>
    </actuator>
    <actuator>
        <velocity name="spring1a_vel" group="2" joint="cw_cont_1a_top" kv="0.1" ctrlrange="-6.28 12.56"/>
        <velocity name="spring2a_vel" group="2" joint="cw_cont_2a_top" kv="0.1" ctrlrange="-6.28 6.28"/>
        <velocity name="spring3a_vel" group="2" joint="cw_cont_3a_btm" kv="0.1" ctrlrange="-6.28 6.28"/>
        <velocity name="spring4a_vel" group="2" joint="cw_cont_4a_btm" kv="0.1" ctrlrange="-6.28 6.28"/>

        <velocity name="spring1c_vel" group="2" joint="cw_ext_1c_top" kv="0.1" ctrlrange="-6.28 6.28"/>
        <velocity name="spring2c_vel" group="2" joint="cw_ext_2c_top" kv="0.1" ctrlrange="-6.28 6.28"/>
        <velocity name="spring3c_vel" group="2" joint="cw_ext_3c_btm" kv="0.1" ctrlrange="-6.28 6.28"/>
        <velocity name="spring4c_vel" group="2" joint="cw_ext_4c_btm" kv="0.1" ctrlrange="-6.28 6.28"/>
    </actuator> 

    <tendon>
        <spatial name="inter1a1" width="0.005" rgba="0 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM1a1"/>
            <site site="siteM1b1"/>
        </spatial>
        <spatial name="inter1a2" width="0.005" rgba="0 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM1a2"/>
            <site site="siteM1b2"/>
        </spatial>
        <spatial name="inter1b1" width="0.005" rgba="0 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM1b3"/>
            <site site="siteM1c1"/>
        </spatial>
        <spatial name="inter1b2" width="0.005" rgba="0 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM1b4"/>
            <site site="siteM1c2"/>
        </spatial>

        <spatial name="inter2a1" width="0.005" rgba="1 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM2a1"/>
            <site site="siteM2b1"/>
        </spatial>
        <spatial name="inter2a2" width="0.005" rgba="1 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM2a2"/>
            <site site="siteM2b2"/>
        </spatial>
        <spatial name="inter2b1" width="0.005" rgba="1 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM2b3"/>
            <site site="siteM2c1"/>
        </spatial>
        <spatial name="inter2b2" width="0.005" rgba="1 1 0 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM2b4"/>
            <site site="siteM2c2"/>
        </spatial>

        <spatial name="inter3a1" width="0.005" rgba="0 1 1 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM3a1"/>
            <site site="siteM3b1"/>
        </spatial>
        <spatial name="inter3a2" width="0.005" rgba="0 1 1 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM3a2"/>
            <site site="siteM3b2"/>
        </spatial>
        <spatial name="inter3b1" width="0.005" rgba="0 1 1 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM3b3"/>
            <site site="siteM3c1"/>
        </spatial>
        <spatial name="inter3b2" width="0.005" rgba="0 1 1 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM3b4"/>
            <site site="siteM3c2"/>
        </spatial>

        <spatial name="inter4a1" width="0.005" rgba="0 1 0.5 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM4a1"/>
            <site site="siteM4b1"/>
        </spatial>
        <spatial name="inter4a2" width="0.005" rgba="0 1 0.5 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM4a2"/>
            <site site="siteM4b2"/>
        </spatial>
        <spatial name="inter4b1" width="0.005" rgba="0 1 0.5 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM4b3"/>
            <site site="siteM4c1"/>
        </spatial>
        <spatial name="inter4b2" width="0.005" rgba="0 1 0.5 1" stiffness="1500" damping="25" limited="true" range="0.2100 0.2600">
            <site site="siteM4b4"/>
            <site site="siteM4c2"/>
        </spatial>
    </tendon>
</mujoco>